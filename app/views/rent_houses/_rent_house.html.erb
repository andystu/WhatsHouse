<li>
  <span class="address">
  <%= rent_house.address %>
  </span>
  <span class="timestamp">
    更新日期<%= rent_house.updated_at.to_s(:long) %>
  </span>
  <% if current_user %>
   <%= link_to "編輯", edit_rent_house_path(rent_house), class:"", 'data-no-turbolink' => true %> 
   <%= link_to "刪除", rent_house_path(rent_house), method: :delete,
    data: { confirm: "You sure?" },
    title: rent_house.address %>
   <% end %>
</li>
